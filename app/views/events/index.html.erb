<div class="header" >
  <h1>.dance</h1>
</div>

<div class="container">
  <div class="index-grid">
    <% if @events.any? %>
      <% @events.each do |event| %>
        <%= render event, event: event %>
      <% end %>
    <% else %>
      <em>Sorry, no events found.</em>
    <% end %>
  </div>
</div>
